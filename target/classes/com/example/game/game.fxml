<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>

<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>
<GridPane xmlns:fx="http://javafx.com/fxml" prefHeight="480" prefWidth="750" fx:controller="com.example.game.LandscapeController">
        <ImageView fx:id="background"/>
            <VBox alignment="TOP_LEFT">
                <Button fx:id="quit" onAction="#onHomeScreen"/>
            </VBox>
        <VBox fx:id="data" alignment="TOP_RIGHT">
            <Label fx:id="moneyText" wrapText="true" textFill="black">
                <padding>
                    <Insets right="20.0" />
                </padding>
                <font>
                    <Font name="Arial Black" size="12"/>
                </font>
            </Label>
        </VBox>


</GridPane>