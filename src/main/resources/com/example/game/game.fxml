<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.game.LandscapeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="10.0"/>
    </padding>
    <top>
        <VBox alignment="TOP_RIGHT">
            <Label fx:id="moneyText" wrapText="true" textFill="black">
                <font>
                    <Font name="Arial Black" size="12"/>
                </font>
            </Label>
        </VBox>
    </top>
</BorderPane>
